<%-include('../header.ejs') %>


    <h1>Display</h1>
    <div class="container mt-5">
        <% if(msg.length > 0){ %>
            <div class="alert alert-success">
                <!-- To Print -->
                <%= msg %>
            </div>
        <% } %>


        <table class="table table-bordered text-center">
            <tr>
                <th>Id</th>
                <th>name</th>
                <th>email</th>
                <th>course</th>
                <th>Status</th>
                <th>Comment</th>
                <th>Action</th>
            </tr>
            <% d.forEach((item)=>{ %>
                <tr>
                    <td>
                        <%= item._id %>
                    </td>
                    <td>
                        <%= item.name %>
                    </td>
                    <td>
                        <%= item.email %>
                    </td>
                    <td>
                        <%= item.course %>
                    </td>
                    <td> <%= item.status %></td>
                    <td> <%= item.comment %></td>


                    <td>
                        <a href="/courseView/<%=item._id%>" class="btn btn-primary">View</a>
                        <a href="/courseEdit/<%=item._id%>" class="btn btn-primary">Edit</a>
                        <!-- <a href="/courseDelete/<%=item._id%>" class="btn btn-primary">Delete</a> -->
                    </td>
                </tr>

                <% }) %>

        </table>
    </div>




    <%-include('../footer.ejs') %>